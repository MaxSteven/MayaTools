{
	"name": "caretaker",
	"version": "0.0.0.1",
	"manifest_version": 2,
	"description": "Send a message to a native application.",
	"background":{
		"scripts":[
			"background.js"]
	},
	"icons": {
		"128": "icon-128.png"
	},
	"permissions": [
		"nativeMessaging"
	],
	"externally_connectable":{
		"matches": ["http://localhost/*", "http://caretaker/*"]
	}
}
